NBAdata<-readr::read_csv("Seasons_Stats.csv")
keanu<-function(year){
NBAdata<-filter(NBAdata, Year==year)
lapply(NBAdata,class)
numdata<-NBAdata[,sapply(NBAdata, is.numeric)]
cormat<-cor(numdata)
print(cormat)
nbacor<-cor(numdata)
cp<-corrplot::corrplot(nbacor, method="color" )
return(cp)
}
mjPoints<-function(year){
dataYear<-dplyr::filter(NBAdata,Year==year)
MJ<-dplyr::filter(dataYear, Player=="Michael Jordan*")
return((dplyr::select(MJ, PTS)))
}
grandpa<-function(year){
yearData<-dplyr::filter(NBAdata, NBAdata$Year==year)
ages<-dplyr::select(yearData, Player, Age)
ages<-dplyr::arrange(ages, desc(Age))
as.data.frame(ages)
return(ages[1,])
}
NBAtools::granpda(1991)
NBAtools::keanu(1991)
NBAtools::mjPoints(1991)
